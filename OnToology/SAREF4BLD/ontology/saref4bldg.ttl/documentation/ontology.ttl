                owl:onProperty bldg:thermalMassHeatCapacity
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:thermalEfficiency
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/pset/pset_spaceheatertypecommon.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/lexical/ifcspaceheater.htm> .

bldg:nominalFilterFaceVelocity
      a       owl:ObjectProperty ;
      rdfs:comment "Filter face velocity. Usually measured in m/s."@en ;
      rdfs:label "nominal filter face velocity"@en .

bldg:firstCurvatureRadius
