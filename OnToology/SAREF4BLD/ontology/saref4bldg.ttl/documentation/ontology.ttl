              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:energySource
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:integer ;
                owl:onProperty bldg:numberOfPanels
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:heatTransferMedium
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
