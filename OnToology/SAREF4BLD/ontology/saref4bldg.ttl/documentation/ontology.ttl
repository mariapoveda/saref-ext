                owl:onProperty bldg:nominalReturnWaterTemperatureCooling
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty bldg:isFreeHanging
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:waterFlowControlSystemType
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
