@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix prov:    <http://www.w3.org/ns/prov#> .
@prefix vann:    <http://purl.org/vocab/vann/> .
@prefix xml:     <http://www.w3.org/XML/1998/namespace> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix saref:   <https://w3id.org/saref#> .
@prefix bldg:    <http://saref.linkeddata.es/def/bldg#> .

bldg:waterStorageCapacity
      a       owl:ObjectProperty ;
      rdfs:comment "Water storage capacity. Usually measured in cubic metre (m3)."@en ;
      rdfs:label "water storage capacity"@en .

bldg:thermalConductivity
      a       owl:ObjectProperty ;
      rdfs:comment "Fouling factor of the tubes in the tube bundle. Usually measured in m2 Kelvin/Watt."@en ;
      rdfs:label "thermal conductivity"@en .

bldg:internalControl
      a       owl:DatatypeProperty ;
      rdfs:comment "Internal modulation control."@en ;
      rdfs:label "internal control"@en ;
      rdfs:range xsd:string .

bldg:SwitchingDevice
      a       owl:Class ;
      rdfs:comment """A switch is used in a cable distribution system (electrical circuit) to control or modulate the flow of electricity.

Switches include those used for electrical power, communications, audio-visual, or other distribution system types as determined by the available ports."""@en ;
      rdfs:label "Switching device"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcSwitchingDevice" ;
      rdfs:subClassOf bldg:FlowController ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty bldg:hasLock
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:integer ;
                owl:onProperty bldg:numberOfGangs
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:legend
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty bldg:isIlluminated
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:switchFunction
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcelectricaldomain/pset/pset_switchingdevicetypecommon.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcelectricaldomain/lexical/ifcswitchingdevice.htm> .

bldg:nominalLengthOrDiameter
      a       owl:ObjectProperty ;
      rdfs:comment "The nominal length or, in the case of a vertical cylindrical tank, the nominal diameter of the tank. Usually measured in millimeters (mm)."@en ;
      rdfs:label "nominal length or diameter"@en .

saref:hasValue
      a       owl:DatatypeProperty ;
      rdfs:comment "A relationship defining the value of a certain property, e.g., energy or power"@en ;
      rdfs:isDefinedBy saref: ;
      rdfs:label "has value"@en .

bldg:netPositiveSuctionHead
      a       owl:ObjectProperty ;
      rdfs:comment "Minimum liquid pressure at the pump inlet to prevent cavitation. Usually measured in Pascals (Pa, N/m2)."@en ;
      rdfs:label "net positive suction head"@en .

bldg:nominalReturnWaterTemperatureHeating
      a       owl:ObjectProperty ;
      rdfs:comment "Nominal return water temperature (refers to nominal heating capacity). Usually measured in degrees Kelvin (K)."@en ;
      rdfs:label "nominal return water temperature heating"@en .

bldg:workingPressureMax
      a       owl:ObjectProperty ;
      rdfs:comment "Maximum working pressure. Usually measured in Pascals (Pa, N/m2)."@en ;
      rdfs:label "maximum working pressure"@en .

bldg:FlowTreatmentDevice
      a       owl:Class ;
      rdfs:comment "The distribution flow element FlowTreatmentDevice defines the occurrence of a device typically used to remove unwanted matter from a fluid, either liquid or gas, and typically participates in a flow distribution system."@en ;
      rdfs:label "Flow treatment device"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcFlowTreatmentDevice" ;
      rdfs:subClassOf bldg:DistributionFlowDevice ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcsharedbldgserviceelements/lexical/ifcflowtreatmentdevice.htm> .

bldg:EnergyConversionDevice
      a       owl:Class ;
      rdfs:comment "The distribution flow element EnergyConversionDevice defines the occurrence of a device used to perform energy conversion or heat transfer and typically participates in a flow distribution system."@en ;
      rdfs:label "Energy conversion device"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcEnergyConversionDevice" ;
      rdfs:subClassOf bldg:DistributionFlowDevice ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcsharedbldgserviceelements/lexical/ifcenergyconversiondevice.htm> .

bldg:lampCompensationType
      a       owl:DatatypeProperty ;
      rdfs:comment "Identifies the form of compensation used for power factor correction and radio suppression."@en ;
      rdfs:label "lamp compensation type"@en ;
      rdfs:range xsd:string .

dcterms:rightsHolder
      a       owl:AnnotationProperty .

bldg:visibleLightTransmittance
      a       owl:ObjectProperty ;
      rdfs:comment "Fraction of the visible light that passes the shading system at normal incidence. It is a value without unit."@en ;
      rdfs:label "visible light transmittance"@en .

bldg:Alarm
      a       owl:Class ;
      rdfs:comment """An alarm is a device that signals the existence of a condition or situation that is outside the boundaries of normal expectation or that activates such a device.

Alarms include the provision of break glass buttons and manual pull boxes that are used to activate alarms."""@en ;
      rdfs:label "Alarm"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcAlarm" ;
      rdfs:subClassOf bldg:DistributionControlDevice ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcbuildingcontrolsdomain/lexical/ifcalarm.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcbuildingcontrolsdomain/pset/pset_alarmtypecommon.htm> .

bldg:Valve
      a       owl:Class ;
      rdfs:comment "A valve is used in a building services piping distribution system to control or modulate the flow of the fluid."@en ;
      rdfs:label "Valve"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcValve" ;
      rdfs:subClassOf bldg:FlowController ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:valveOperation
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:valvePattern
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:workingPressure
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:size
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:valveMechanism
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:flowCoefficient
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:testPressure
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:closeOffRating
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/lexical/ifcvalve.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/pset/pset_valvetypecommon.htm> .

bldg:isNeutralSecondaryTerminalAvailable
      a       owl:DatatypeProperty ;
      rdfs:comment "An indication of whether the neutral point of the secondary winding is available as a terminal (=TRUE) or not (= FALSE)."@en ;
      rdfs:label "is neutral secondary terminal available"@en ;
      rdfs:range xsd:boolean .

bldg:airFlowRateMax
      a       owl:ObjectProperty ;
      rdfs:comment "Maximum allowable air flow rate. Usually measured in m3/s."@en ;
      rdfs:label "air flow rate range"@en .

bldg:solarReflectance
      a       owl:ObjectProperty ;
      rdfs:comment "(Rsol): The ratio of incident solar radiation that is reflected by a shading system (also named _e). Note the following equation Asol + Rsol + Tsol = 1"@en ;
      rdfs:label "solar reflectance"@en .

bldg:DistributionFlowDevice
      a       owl:Class ;
      rdfs:comment "Represents devices of a distribution system that facilitate the distribution of energy or matter, such as air, water or power. (Definition adapated from http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcsharedbldgserviceelements/lexical/ifcdistributionflowelement.htm)"@en ;
      rdfs:label "DistributionFlowDevice"@en ;
      rdfs:subClassOf bldg:DistributionDevice .

bldg:hasTimestamp
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:label "has timestamp"@en ;
      rdfs:range xsd:dateTime .

bldg:PhysicalObject
      a       owl:Class ;
      rdfs:comment "Any Object that has a proper space region.  (Definition extracted from DUL ontology)"@en ;
      rdfs:label "Physical object"@en ;
      rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:PhysicalObject ;
                owl:onProperty bldg:contains
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (bldg:BuildingSpace bldg:PhysicalObject)
                        ] ;
                owl:onProperty bldg:isContainedIn
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
                owl:onProperty <http://www.w3.org/2003/01/geo/wgs84_pos#location>
              ] .

bldg:partLoadRatioMin
      a       owl:ObjectProperty ;
      rdfs:comment "Minimum part load ratio as a fraction of nominal capacity."@en ;
      rdfs:label "minimum part load ratio"@en .

bldg:Evaporator
      a       owl:Class ;
      rdfs:comment "An evaporator is a device in which a liquid refrigerent is vaporized and absorbs heat from the surrounding fluid."@en ;
      rdfs:label "Evaporator"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcEvaporator" ;
      rdfs:subClassOf bldg:EnergyConversionDevice ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:internalWaterVolume
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:refrigerantClass
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:evaporationMediumType
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:nominalHeatTransferArea
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:externalSurfaceArea
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:internalSurfaceArea
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:internalRefrigerantVolume
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:evaporationCoolant
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:nominalHeatTransferCoefficient
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/pset/pset_evaporatortypecommon.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/lexical/ifcevaporator.htm> .

bldg:numberOfPanels
      a       owl:DatatypeProperty ;
      rdfs:comment "Number of panels."@en ;
      rdfs:label "number of panels"@en ;
      rdfs:range xsd:integer .

bldg:EvaporativeCooler
      a       owl:Class ;
      rdfs:comment "An evaporative cooler is a device that cools air by saturating it with water vapor."@en ;
      rdfs:label "Evaporative cooler"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcEvaporativeCooler" ;
      rdfs:subClassOf bldg:EnergyConversionDevice ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:operationTemperatureMin
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:waterRequirement
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:operationTemperatureMax
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:heatExchangeArea
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:flowArrangement
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/pset/pset_evaporativecoolertypecommon.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/lexical/ifcevaporativecooler.htm> .

bldg:nominalAirFlowRate
      a       owl:ObjectProperty ;
      rdfs:comment "Nominal rate of air flow. Usually measured in m3/s."@en ;
      rdfs:label "nominal air flow rate"@en .

bldg:nominalSorroundingTemperatureCooling
      a       owl:ObjectProperty ;
      rdfs:comment "Nominal surrounding temperature (refers to nominal cooling capacity). Usually measured in degrees Kelvin (K)."@en ;
      rdfs:label "nominal sorrounding temperature cooling"@en .

bldg:integratedLightingType
      a       owl:DatatypeProperty ;
      rdfs:comment "Integrated lighting in cooled beam."@en ;
      rdfs:label "integrated lighting type"@en ;
      rdfs:range xsd:string .

bldg:isolatorStaticDeflection
      a       owl:ObjectProperty ;
      rdfs:comment "Static deflection of the vibration isolator. Usually measured in millimeters (mm)."@en ;
      rdfs:label "isolator static deflection"@en .

bldg:Building
      a       owl:Class ;
      rdfs:comment "A building represents a structure that provides shelter for its occupants or contents and stands in one place. The building is also used to provide a basic element within the spatial structure hierarchy for the components of a building project (together with site, storey, and space)."@en ;
      rdfs:label "Building"@en ;
      rdfs:seeAlso <https://w3id.org/ifc/IFC4_ADD1#IfcBuilding> ;
      rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
                owl:onProperty <http://www.w3.org/2003/01/geo/wgs84_pos#location>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:BuildingSpace ;
                owl:onProperty bldg:hasSpace
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcproductextension/lexical/ifcbuilding.htm> .

saref:Device
      a       owl:Class ;
      rdfs:comment "A tangible object designed to accomplish a particular task in households, common public buildings or offices. In order to accomplish this task, the device performs one or more functions. For example, a washing machine is designed to wash (task) and to accomplish this task it performs the start and stop function."@en ;
      rdfs:isDefinedBy saref: ;
      rdfs:label "Device"@en ;
      rdfs:subClassOf bldg:PhysicalObject .

bldg:Tank
      a       owl:Class ;
      rdfs:comment "A tank is a vessel or container in which a fluid or gas is stored for later use."@en ;
      rdfs:label "Tank"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcTank" ;
      rdfs:subClassOf bldg:FlowStorageDevice ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:nominalVolumetricCapacity
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:nominalDepth
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:firstCurvatureRadius
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:storageType
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:nominalLengthOrDiameter
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:patternType
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:nominalWidthOrDiameter
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:operatingWight
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:effectiveCapacity
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:endShapeType
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:integer ;
                owl:onProperty bldg:numberOfSections
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:accessType
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:secondCurvatureRadius
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/pset/pset_tanktypecommon.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifchvacdomain/lexical/ifctank.htm> .

bldg:Actuator
      a       owl:Class ;
      rdfs:comment "An actuator is a mechanical device for moving or controlling a mechanism or system. An actuator takes energy, usually created by air, electricity, or liquid, and converts that into some kind of motion."@en ;
      rdfs:label "Actuator"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcActuator" ;
      rdfs:subClassOf saref:Actuator , bldg:DistributionControlDevice ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty bldg:failPosition
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:boolean ;
                owl:onProperty bldg:manualOverride
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcbuildingcontrolsdomain/lexical/ifcactuator.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcbuildingcontrolsdomain/pset/pset_actuatortypecommon.htm> .

bldg:frameDepth
      a       owl:ObjectProperty ;
      rdfs:comment "The length (or depth) of the damper frame. Usually measured in millimeters (mm)."@en ;
      rdfs:label "frame depth"@en .

bldg:length
      a       owl:ObjectProperty ;
      rdfs:comment "The finished length of the device. Usually measured in millimeters (mm)."@en ;
      rdfs:label "length"@en .

bldg:coverLength
      a       owl:ObjectProperty ;
      rdfs:comment "The length measured along the x-axis in the local coordinate system or the radius (in the case of a circular shape in plan) of the cover of the oil interceptor. Usually measured in millimeters (mm)."@en ;
      rdfs:label "cover length"@en .

bldg:nominalWaterFlowHeating
      a       owl:ObjectProperty ;
      rdfs:comment "Nominal water flow (refers to nominal heating capacity). Usually measured in m3/s."@en ;
      rdfs:label "nominal water flow heating"@en .

bldg:frameSize
      a       owl:DatatypeProperty ;
      rdfs:comment "Designation of the frame size according to the named range of frame sizes designated at the place of use or according to a given standard."@en ;
      rdfs:label "frame size"@en ;
      rdfs:range xsd:string .

bldg:nominalPowerRate
      a       owl:ObjectProperty ;
      rdfs:comment "Nominal fan power rate.Usually measured in Watts (W, J/s)."@en ;
      rdfs:label "nominal power rate"@en .

bldg:liftElevationDifference
      a       owl:ObjectProperty ;
      rdfs:comment "Elevation difference between cooling tower sump and the top of the tower. Usually measured in millimeters (mm)."@en ;
      rdfs:label "lift elevation difference"@en .

bldg:nominalWidthOrDiameter
      a       owl:ObjectProperty ;
      rdfs:comment "The nominal width or, in the case of a horizontal cylindrical tank, the nominal diameter of the tank. Note: Not required for a vertical cylindrical tank. Usually measured in millimeters (mm)."@en ;
      rdfs:label "nominal width or diameter"@en .

bldg:ElectricGenerator
      a       owl:Class ;
      rdfs:comment "An electric generator is an engine that is a machine for converting mechanical energy into electrical energy."@en ;
      rdfs:label "Electric generator"@en ;
      rdfs:seeAlso "https://w3id.org/ifc/IFC4_ADD1#IfcElectricGenerator" ;
      rdfs:subClassOf bldg:EnergyConversionDevice ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:startCurrentFactor
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:powerOutputMax
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom bldg:Measurement ;
                owl:onProperty bldg:electricGeneratorEfficiency
              ] ;
      prov:hadPrimarySource
              <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcelectricaldomain/lexical/ifcelectricgenerator.htm> , <http://www.buildingsmart-tech.org/ifc/IFC4/Add1/html/schema/ifcelectricaldomain/pset/pset_electricgeneratortypecommon.htm> .

bldg:pipeConnectionEnum
      a       owl:DatatypeProperty ;
      rdfs:comment "The manner in which the pipe connection is made to the cooled beam."@en ;
      rdfs:label "pipe connection enum"@en ;
      rdfs:range xsd:string .

bldg:leakageFullyClosed
      a       owl:ObjectProperty ;
      rdfs:comment "Leakage when fully closed. Usually measured in m3/s."@en ;
      rdfs:label "leakage fully closed"@en .

bldg:accessType
      a       owl:DatatypeProperty ;
      rdfs:comment "Defines the types of access (or cover) to a tank that may be specified. Note that covers are generally specified for rectangular tanks. For cylindrical tanks, access will normally be via a manhole."@en ;
      rdfs:label "access type"@en ;
      rdfs:range xsd:string .

bldg:nominalEvaporatingTemmperature
      a       owl:ObjectProperty ;
      rdfs:comment "Chiller evaporating temperature.Usually measured in degrees Kelvin (K)."@en ;
