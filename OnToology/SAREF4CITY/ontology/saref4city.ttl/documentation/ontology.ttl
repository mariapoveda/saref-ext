@base <https://w3id.org/def/saref4city> .

<https://w3id.org/def/saref4city> rdf:type owl:Ontology ;
                                   owl:versionIRI <https://w3id.org/def/v1/saref4city> ;
                                   <http://purl.org/dc/elements/1.1/description> "This ontology extends the SAREF ontology for the Smart Cities domain. This work has been developed in the context of the STF 534 (https://portal.etsi.org/STF/STFs/STFHomePages/STF534.aspx), which was established with the goal to create three SAREF extensions, one of them for the Smart Cities domain."@en ;
                                   <http://purl.org/dc/elements/1.1/publisher> <http://www.oeg-upm.net/> ;
                                   <http://purl.org/dc/elements/1.1/title> "SAREF extension for Smart City"@en ;
                                   <http://purl.org/dc/terms/creator> <http://w3id.org/people/mpoveda> ,
                                                                      <http://www.garcia-castro.com/foaf.rdf#me> ;
                                   <http://purl.org/dc/terms/license> <http://purl.org/NET/rdflicense/cc-by4.0> ;
                                   <http://purl.org/vocab/vann/preferredNamespacePrefix> "s4city"^^xsd:string ;
                                   <http://purl.org/vocab/vann/preferredNamespaceUri> "https://w3id.org/def/saref4city#"^^xsd:string ;
                                   owl:versionInfo 0.1 .

