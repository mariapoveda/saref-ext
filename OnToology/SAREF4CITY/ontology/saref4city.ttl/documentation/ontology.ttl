                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty <https://w3id.org/saref#accomplishes> ;
                                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass <https://w3id.org/saref#Task>
